@SUFFIX_LINE@


SHELL = /bin/sh

@SET_MAKE@

CXX = @CXX@
CXXFLAGS = @CXXFLAGS@

LDFLAGS = @LDFLAGS@
LIBS = @LIBS@
RANLIB = @RANLIB@

BX_INCDIRS = @DASH@I.. -I../@INSTRUMENT_DIR@
LOCAL_CXXFLAGS =

X86_OBJS = x86.o x86shim.o
X86_H    = x86.h

SPARC_OBJS =
SPARC_H    =

BX_DYNAMIC_OBJS = dynamic.o @AS_DYNAMIC_OBJS@


.@CPP_SUFFIX@.o:
	$(CXX) @DASH@c $(CXXFLAGS) $(LOCAL_CXXFLAGS) $(BX_INCDIRS) @CXXFP@$< @OFP@$@



libdynamic.a: $(BX_DYNAMIC_OBJS)
	@RMCOMMAND@ libdynamic.a
	@MAKELIB@ $(BX_DYNAMIC_OBJS)
	@RANLIB@ libdynamic.a

$(BX_DYNAMIC_OBJS): dynamic.h @AS_DYNAMIC_INCS@

clean:
	@RMCOMMAND@ *.o
	@RMCOMMAND@ *.a

dist-clean: clean
	@RMCOMMAND@ Makefile
