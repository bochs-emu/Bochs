# Makefile for the gui component of bochs


@SUFFIX_LINE@

srcdir = @srcdir@
VPATH = @srcdir@

SHELL = /bin/sh

@SET_MAKE@

CXX = @CXX@
CXXFLAGS = @CXXFLAGS@  `wx-config --cxxflags`

LDFLAGS = @LDFLAGS@
LIBS = @LIBS@
X_CFLAGS = @X_CFLAGS@
BEOS_CFLAGS = -Wno-multichar
RFB_CFLAGS =
SDL_CFLAGS =
RANLIB = @RANLIB@

GUI_OBJS_X11  = x.o
GUI_OBJS_SDL = sdl.o
GUI_OBJS_BEOS = beos.o
GUI_OBJS_WIN32 = win32.o
GUI_OBJS_MACOS = macintosh.o
GUI_OBJS_CARBON = carbon.o
GUI_OBJS_NOGUI = nogui.o
GUI_OBJS_TERM  = term.o
GUI_OBJS_RFB = rfb.o
GUI_OBJS_AMIGAOS = amigaos.o
GUI_OBJS_WX =
GUI_OBJS = keymap.o gui.o siminterface.o @GUI_OBJS@
# note control.o should not be used with wxwindows, so I removed it here.
# control.o is only for text mode interface.


BX_INCDIRS = -I.. -I$(srcdir)/.. -I../iodev -I$(srcdir)/../iodev -I../@INSTRUMENT_DIR@ -I$(srcdir)/../@INSTRUMENT_DIR@
LOCAL_CXXFLAGS =

#
# -------- end configurable options --------------------------
#

BX_GUI_OBJS = $(GUI_OBJS)


BX_INCLUDES = ../iodev/iodev.h control.h siminterface.h

.@CPP_SUFFIX@.o:
	$(CXX) @DASH@c $(CXXFLAGS) $(LOCAL_CXXFLAGS) $(BX_INCDIRS) @CXXFP@$< @OFP@$@



libgui.a: $(BX_GUI_OBJS)
	@RMCOMMAND@ libgui.a
	@MAKELIB@ $(BX_GUI_OBJS)
	$(RANLIB) libgui.a

$(BX_GUI_OBJS) : $(BX_INCLUDES)

beos.o: beos.@CPP_SUFFIX@
	$(CXX) -c $(CXXFLAGS) $(LOCAL_CXXFLAGS) $(BX_INCDIRS) $(BEOS_CFLAGS) $<

x.o: x.@CPP_SUFFIX@
	$(CXX) -c $(CXXFLAGS) $(LOCAL_CXXFLAGS) $(BX_INCDIRS) $(X_CFLAGS) $<

sdl.o: sdl.@CPP_SUFFIX@ sdl.h
	$(CXX) -c $(CXXFLAGS) $(LOCAL_CXXFLAGS) $(BX_INCDIRS) $(SDL_CFLAGS) $<

rfb.o: rfb.@CPP_SUFFIX@ rfb.h rfbproto.h
	$(CXX) -c $(CXXFLAGS) $(LOCAL_CXXFLAGS) $(BX_INCDIRS) $(RFB_CFLAGS) $<

x.@CPP_SUFFIX@cc: gui.h

beos.@CPP_SUFFIX@cc: gui.h

clean:
	@RMCOMMAND@ *.o
	@RMCOMMAND@ *.a

dist-clean: clean
	@RMCOMMAND@ Makefile

