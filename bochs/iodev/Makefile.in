# Makefile for the iodev component of bochs


@SUFFIX_LINE@

srcdir = @srcdir@
VPATH = @srcdir@
top_builddir    = ..
top_srcdir      = @top_srcdir@

SHELL = /bin/sh

@SET_MAKE@

CXX = @CXX@
CXXFLAGS = $(BX_INCDIRS) @CXXFLAGS@ @GUI_CXXFLAGS@

LDFLAGS = @LDFLAGS@
LIBS = @LIBS@
RANLIB = @RANLIB@
PLUGIN_PATH=@libdir@
top_builddir    = ..
LIBTOOL=@LIBTOOL@
WIN32_DLL_IMPORT_LIBRARY=../dllexports.a

CDROM_OBJS = @CDROM_OBJS@
IOAPIC_OBJS = @IOAPIC_OBJS@

BX_INCDIRS = -I.. -I$(srcdir)/.. -I../@INSTRUMENT_DIR@ -I$(srcdir)/../@INSTRUMENT_DIR@
LOCAL_CXXFLAGS = $(MCH_CFLAGS)

SB16_DUMMY_OBJS = sb16.o
SB16_LINUX_OBJS = sb16.o soundlnx.o
SB16_WIN_OBJS   = sb16.o soundwin.o
SLOWDOWN_OBJS = slowdown_timer.o

OBJS_THAT_CANNOT_BE_PLUGINS = \
  devices.o \
  pit.o pit82c54.o pit_wrap.o \
  @IODEBUG_OBJS@ \
  @PCI_OBJ@ \
  @NE2K_OBJS@ \
  @SB16_OBJS@ \
  @SLOWDOWN_OBJS@ \
  $(MCH_OBJS) \
  $(IOAPIC_OBJS)

OBJS_THAT_CAN_BE_PLUGINS = \
  pic.o \
  serial.o \
  parallel.o \
  floppy.o \
  keyboard.o \
  vga.o \
  biosdev.o \
  cmos.o \
  harddrv.o \
  dma.o \
  unmapped.o 

OBJS_THAT_SUPPORT_OTHER_PLUGINS = \
  scancodes.o \
  $(CDROM_OBJS)

NONPLUGIN_OBJS = @IODEV_NON_PLUGIN_OBJS@
PLUGIN_OBJS = @IODEV_PLUGIN_OBJS@

all: libiodev.a 

plugins: $(PLUGIN_OBJS:@PLUGIN_LIBNAME_TRANSFORMATION@)

libiodev.a: $(NONPLUGIN_OBJS)
	@RMCOMMAND@ libiodev.a
	@MAKELIB@ $(NONPLUGIN_OBJS)
	@RANLIB@ libiodev.a

# standard compile rule for C++ files
%.o: %.@CPP_SUFFIX@
	$(CXX) -c $(CXXFLAGS) $(LOCAL_CXXFLAGS) $< -o $@

##### building plugins with libtool
%.lo: %.@CPP_SUFFIX@
	$(LIBTOOL) $(CXX) -c $(CXXFLAGS) $(LOCAL_CXXFLAGS) $< -o $@

lib%.la: %.lo
	$(LIBTOOL) $(CXX) -module $< -o $@ -rpath $(PLUGIN_PATH)

# special link rules for plugins that require more than one object file
libharddrv.la: harddrv.lo $(CDROM_OBJS:.o=.lo)
	$(LIBTOOL) $(CXX) -module harddrv.lo $(CDROM_OBJS:.o=.lo) -o libharddrv.la -rpath $(PLUGIN_PATH)

libkeyboard.la: keyboard.lo scancodes.lo
	$(LIBTOOL) $(CXX) -module keyboard.lo scancodes.lo -o libkeyboard.la -rpath $(PLUGIN_PATH)

libpit.la: pit.lo pit82c54.lo pit_wrap.lo
	$(LIBTOOL) $(CXX) -module pit.lo pit82c54.lo pit_wrap.lo -o libpit.la -rpath $(PLUGIN_PATH)

#### building DLLs for win32  (tested on cygwin only)
%.dll: %.o
	$(CXX) $(CXXFLAGS) -shared -o $@ $< $(WIN32_DLL_IMPORT_LIBRARY)

# special link rules for plugins that require more than one object file
harddrv.dll: harddrv.o $(CDROM_OBJS)
	$(CXX) $(CXXFLAGS) -shared -o harddrv.dll harddrv.o $(CDROM_OBJS) $(WIN32_DLL_IMPORT_LIBRARY)

keyboard.dll: keyboard.o scancodes.o
	$(CXX) $(CXXFLAGS) -shared -o keyboard.dll keyboard.o scancodes.o $(WIN32_DLL_IMPORT_LIBRARY)

pit.dll: pit.o pit82c54.o pit_wrap.o
	$(CXX) $(CXXFLAGS) -shared -o pit.dll pit.o pit82c54.o pit_wrap.o $(WIN32_DLL_IMPORT_LIBRARY)
##### end DLL section

clean:
	@RMCOMMAND@ -rf .libs *.lo *.o *.la *.a *.dll

dist-clean: clean
	@RMCOMMAND@ Makefile

