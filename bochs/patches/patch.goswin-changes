+ 	@CD_UP_ONE@
+ 	cd debug @COMMAND_SEPARATOR@
+ 	$(MAKE) clean
+ 	@CD_UP_ONE@
+ 	cd cpu @COMMAND_SEPARATOR@
+ 	$(MAKE) clean
+ 	@CD_UP_ONE@
+ 	cd memory @COMMAND_SEPARATOR@
+ 	$(MAKE) clean
+ 	@CD_UP_ONE@
+ 	cd gui @COMMAND_SEPARATOR@
+ 	$(MAKE) clean
+ 	@CD_UP_ONE@
+ 	cd disasm @COMMAND_SEPARATOR@
+ 	$(MAKE) clean
+ 	@CD_UP_ONE@
