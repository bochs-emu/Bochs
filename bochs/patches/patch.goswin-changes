  AC_SUBST(CD_UP_TWO)
  
+ LIBS="$X_PRE_LIBS $X_LIBS $X_EXTRA_LIBS $LIBS"
  
! AC_OUTPUT(Makefile \
! 	bios/Makefile \
!  	cpu/Makefile \
! 	debian/Makefile \
