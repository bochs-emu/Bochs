! 	instrument/example0/Makefile \
! 	instrument/stubs/Makefile \
! dnl 	${INSTRUMENT_DIR}/Makefile \
! 	iodev/Makefile \
! 	memory/Makefile \
! 	misc/Makefile \
! )
