+++ bochs-paste/gui/keymaps/convertmap.pl	Mon Mar 11 09:23:44 2002
@@ -0,0 +1,10 @@
+#!/usr/bin/perl
+
+while (<STDIN>)
+{
+  chop;
+  s/^ *//;
+  if (/^#/ || /^ *$/) { print "$_\n"; next;}
+  ($key, $equals, $xksym) = split (/ +/);
+  printf ("%-45s %-10s %s\n", $key, 'none', "XK_$xksym");
+}
