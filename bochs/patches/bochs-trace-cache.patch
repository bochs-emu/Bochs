+      if (dbg_instruction_epilog()) return;
 #endif
 
-    CHECK_MAX_INSTRUCTIONS(max_instr_count);
+      CHECK_MAX_INSTRUCTIONS(max_instr_count);
+
+#if BX_SUPPORT_TRACE_CACHE
