+  trace->pAddr = pAddr;
+  trace->writeStamp = ICacheWriteStampInvalid;
+  trace->ilen = 0;
+
+  InstrICache_Increment(iCacheMisses);
+
   bx_address remainingInPage = (BX_CPU_THIS_PTR eipPageWindowSize - eipBiased);
