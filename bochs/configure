  "memory/Makefile" ) CONFIG_FILES="$CONFIG_FILES memory/Makefile" ;;
  "gui/Makefile" ) CONFIG_FILES="$CONFIG_FILES gui/Makefile" ;;
  "disasm/Makefile" ) CONFIG_FILES="$CONFIG_FILES disasm/Makefile" ;;
