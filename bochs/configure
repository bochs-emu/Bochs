      wx_multi_lib=1
      case x$WX_BASENAME in
        xwx_gtk2|xwx_gtk2u) wx_needs_gdk_version=2 ;;
        xwx_gtk3|xwx_gtk3u) wx_needs_gdk_version=3 ;;
        *) ;;
      esac
      ;;
    *) ;; # who knows?
  esac

fi
