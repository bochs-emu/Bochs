	fi
	if test x"$ok" != xPTHREAD_CREATE_JOINABLE; then

printf "%s\n" "#define PTHREAD_CREATE_JOINABLE \$ok" >>confdefs.h

	fi
	{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: ${ok}" >&5
