    if test "${lt_cv_archive_cmds_need_lc+set}" = set; then
  echo $ECHO_N "(cached) $ECHO_C" >&6
else
  $rm conftest*
    echo 'static int dummy;' > conftest.$ac_ext

    if { (eval echo "$as_me:6106: \"$ac_compile\"") >&5
  (eval $ac_compile) 2>&5
  ac_status=$?
  echo "$as_me:6109: \$? = $ac_status" >&5
  (exit $ac_status); }; then
      soname=conftest
