	{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking if more special flags are required for pthreads" >&5
printf %s "checking if more special flags are required for pthreads... " >&6; }
	flag=no
	case "${host_cpu}-${host_os}" in
